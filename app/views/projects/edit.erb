<h2> Edit your project</h2>
<% if @errors  %>
    <ul class="red-text text-darken-2">
    <% @errors.each do |error| %>
     <li><%= error %></li>
    <% end %>
</ul> 

<form action="/projects/<%= @project.id %>" method="post">
    <input type="hidden" value="patch" name="_method">
    <label for="title">Title of project</label>
    <input type="text" name="project[title]" id="title" value="<%= @project.title %>"><br><br>
    <label for="genre">Genre of project</label>
    <input type="text" name="project[genre]" id="genre" value="<%= @project.genre %>"><br><br>
    <label for="info">Some information about the project</label>
    <input type="text" name="project[info]" id="info" value="<%= @project.info %>"><br><br>
    <input type="submit" value="Update Project" class="btn">
</form>